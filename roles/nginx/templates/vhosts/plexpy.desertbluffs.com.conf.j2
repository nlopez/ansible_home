server {
  listen 443{{ " http2" if http2 else "" }} ssl;
  listen [::]:443{{ " http2" if http2 else "" }} ssl;
  server_name plexpy.desertbluffs.com;

  ssl_certificate /usr/local/etc/simp_le/output/plexpy.desertbluffs.com/fullchain.pem;
  ssl_certificate_key /usr/local/etc/simp_le/output/plexpy.desertbluffs.com/key.pem;

  location / {
    include auth.conf;
    include proxy.conf;
    proxy_pass http://192.168.239.6:8181/;
  }

  location /_oauth {
    include auth.conf;
  }
}
