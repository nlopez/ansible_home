server {
  listen 443 ssl;
  listen [::]:443 ssl;
  server_name nzbget.desertbluffs.com;

  ssl_certificate /usr/local/etc/simp_le/output/nzbget.desertbluffs.com/fullchain.pem;
  ssl_certificate_key /usr/local/etc/simp_le/output/nzbget.desertbluffs.com/key.pem;

  location / {
    include auth.conf;
    include proxy.conf;
    proxy_pass http://192.168.239.6:6789/;
    client_max_body_size 5m;
  }

  location /_oauth {
    include auth.conf;
  }
}


