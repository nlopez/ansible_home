server {
  listen 443 ssl;
  listen [::]:443 ssl;
  server_name router.desertbluffs.com;

  location / {
    include auth.conf;
    include proxy.conf;
    proxy_pass http://192.168.239.1/;
    proxy_set_header Authorization "{{router_authorization_header}}";
  }

  location /_oauth {
    include auth.conf;
  }
}
